@model CM_APPLICATIONS.Models.CopRunningModel

@{
    ViewBag.Title = "COP TENSILE REPORT";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var row = 0;
}
<style id="style_local">

    table {
        border-collapse: collapse;
        border-spacing: 0;
    }
</style>
<style>
    .dataTables_wrapper {
        float: left !important;
        width: 99% !important;
        margin-left: 0px !important;
    }

    table.dataTable thead tr {
        background-color: #005495;
        color: #fff;
    }

    table.dataTable {
        margin-left: 0px !important;
        width: 100% !important;
    }
    .nowrap-td {
        white-space: nowrap;
    }
</style>
<h2>ECE R16 TENSILE Report</h2>

<p>
    @Html.ActionLink("UPLOADING SPC FILE (1)", "TensileUpload", "COPR16_COPRUNNING", new { @class = "btn btn-primary button" })

    @Html.ActionLink("UPLOADING PART LIST TEMPLATE FILE (3)", "PartListTemplateUpload", "COPR16_COPRUNNING", new { @class = "btn btn-primary button" })
</p>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.Hidden("UID", CM_APPLICATIONS.Models.AppPropModel.username)
    <div class="form-horizontal">
        <div class="form-group">
            @Html.Label("MODEL NO : ", htmlAttributes: new { @class = "control-label col-md-1", @style = "white-space:nowrap;" })
            <div class="col-md-10" style="display:flex;flex-wrap:nowrap">
                @Html.DropDownList("SPC_MODEL_LIST", Model.SPC_MODEL_LIST, null, new { @class = "form-control MODEL_ID" })
                @Html.Hidden("hidden_MODELNO", "")
            </div>
        </div>
        <div class="form-group">
            @Html.Label("YEAR : ", htmlAttributes: new { @class = "control-label col-md-1", @style = "white-space:nowrap;" })
            <div class="col-md-10" style="display:flex;flex-wrap:nowrap">
                @Html.DropDownList("SPC_YEAR", Model.SPC_YEAR, null, new { @class = "form-control YEAR_SEL" })
                @Html.Hidden("hidden_YEARSEL", "")
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-1 col-md-10">
                <input type="button" value="ENABLE EDIT PARTS" class="btn btn-warning btnEditPart" />
                @*<input type="button" value="SAVE PARTS" class="btn btn-warning btnSavePart" />*@
                <input type="button" value="READ PART LIST FROM TEMPLATE (4)" class="btn btn-primary button readTemplateBtn" />
                <input type="button" value="READ SPC DATA (5)" class="btn btn-primary button refreshBtn" />
                <input type="button" value="EXPORT TO XLS (2,6)" class="btn btn-primary button exportBtn" />
            </div>
        </div>
    </div>
}


